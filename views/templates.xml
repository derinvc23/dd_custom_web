<odoo>
    <data>
         <template id="assets_frontend" inherit_id="website.assets_frontend" name="Custom Module">
             <xpath expr="." position="inside">
                 <scrpt type="text/javascript" src="/dd_custom_web/static/src/js/website.js"></scrpt>
                 <link rel="stylesheet" href="/dd_custom_web/static/src/css/style.css"/>
             </xpath>
         </template>
    </data>
</odoo>
